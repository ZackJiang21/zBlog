<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zack.zblog.dao.UserDao">
    <select id="getUser" resultType="com.zack.zblog.model.User">
        SELECT NAME, VALUE FROM T_USER WHERE NAME = #{name} AND VALUE = #{value}
    </select>

</mapper>